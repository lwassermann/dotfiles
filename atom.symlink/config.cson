"*":
  core:
    audioBeep: false
    disabledPackages: [
      "exception-reporting"
      "metrics"
    ]
    hideGitIgnoredFiles: true
    ignoredNames: [
      ".DS_Store"
      ".bundle"
      ".git"
      ".hg"
      ".svn"
      "log"
      "tmp"
      "vendor"
    ]
    telemetryConsent: "no"
  editor:
    fontFamily: "Menlo"
    fontSize: 12
    invisibles: {}
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
